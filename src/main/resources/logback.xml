<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="console" class="com.qunar">
        <encoder>
            <pattern>[%d{yyyy-MM-dd HH:mm:ss}][%thread][%p][%c{0}:%M:%L] %m%n</pattern>
        </encoder>
    </appender>



    <root level="ERROR">
        <appender-ref ref="console"/>
    </root>

</configuration>